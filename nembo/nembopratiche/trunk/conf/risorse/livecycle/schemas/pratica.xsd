<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xfa="http://www.adobe.com/2003/xfa" xmlns:wmh="http://www.wmhelp.com/2003/eGenerator" elementFormDefault="qualified">
  <xs:element name="Domanda">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Global"/>
        <xs:element ref="Header"/>
        <xs:element ref="QuadroDatiIdentificativi"/>
		<xs:element ref="QuadroContoCorrente"/>
		<xs:element ref="QuadroAnticipo"/>
		<xs:element ref="QuadroInterventi"/>
		<xs:element ref="QuadroAvvioCorsi"/>
        <xs:element ref="QuadroEconomico"/>
		<xs:element ref="QuadroColtureAziendali"/>
        <xs:elemetn ref="QuadroPLVVegetale"/>
		<xs:element ref="QuadroPLVZootecnica"/>
		<xs:element ref="QuadroScorte"/>
		<xs:element ref="QuadroDanni"/>
		<xs:element ref="QuadroDichiarazioni"/>
        <xs:element ref="QuadroImpegni"/>
        <xs:element ref="QuadroAllegati"/>
		<xs:element ref="QuadroAttestazioniCAA"/>
		<xs:element ref="QuadroPremio"/>
		<xs:element ref="QuadroPartecipanti"/>
		<xs:element ref="QuadroParticelleImpegnoGr"/>
		<xs:element ref="QuadroPartGrafImpegnoRinunciate"/>
		<xs:element ref="QuadroDatiCaniGuardianiaInv"/>
		<xs:element ref="QuadroSostRazze"/>
		<xs:element ref="QuadroBorgate"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Global">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Bozza"/>
		<xs:element ref="IsValidazioneGrafica"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Bozza" type="xs:boolean"/>
  <xs:element name="IsValidazioneGrafica" type="xs:boolean"/>
  <xs:element name="Visibility" type="xs:boolean"/>
  <xs:element name="FlagObbligatorio" type="xs:boolean"/>
  <xs:element name="FlagSelezionato" type="xs:boolean"/>
  <xs:element name="Header">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="LogoCentrale"/>
        <xs:element ref="Titolo1"/>
        <xs:element ref="Titolo2"/>
        <xs:element ref="Titolo3"/>
        <xs:element ref="Bando"/>
        <xs:element ref="Operazioni"/>
        <xs:element ref="Oggetto"/>
        <xs:element ref="Specificita"/>
        <xs:element ref="Castelletto"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="LogoCentrale" type="xs:base64Binary"/>
  <xs:element name="Titolo1" type="xs:string"/>
  <xs:element name="Titolo2" type="xs:string"/>
  <xs:element name="Titolo3" type="xs:string"/>
  <xs:element name="Bando" type="xs:string"/>
  <xs:element name="Oggetto" type="xs:string"/>
  <xs:element name="Operazioni" type="xs:string"/>
  <xs:element name="Specificita" type="xs:string"/>
  <xs:element name="Castelletto">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Visibility"/>
        <xs:element ref="Protocollo"/>
        <xs:element ref="IntestazioneBoxRichiedente"/>
        <xs:element ref="CodiceFiscaleTramite"/>
        <xs:element ref="DescrizioneTramite"/>
        <xs:element ref="NumeroDomanda"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Protocollo" type="xs:string"/>
  <xs:element name="IntestazioneBoxRichiedente" type="xs:string"/>
  <xs:element name="CodiceFiscaleTramite" type="xs:string"/>
  <xs:element name="DescrizioneTramite" type="xs:string"/>    
  <xs:element name="NumeroDomanda" type="xs:string"/>
  
  <!-- QUADRO Dati Identificativi  -->
  <xs:element name="QuadroDatiIdentificativi">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SezioneAnagrafica"/>
        <xs:element ref="SezioneTitolare"/>
        <xs:element ref="SezioneFirmatario"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="SezioneAnagrafica">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Visibility"/>
        <xs:element ref="Titolo1sezAnagrafica"/>
        <xs:element ref="Titolo2sezAnagrafica"/>
        <xs:element ref="Cuaa"/>
        <xs:element ref="PartitaIva"/>
        <xs:element ref="IntestazionePartitaIva"/>
        <xs:element ref="Denominazione"/>
        <xs:element ref="FormaGiuridica"/>
        <xs:element ref="IndirizzoSedeLeg"/>
        <xs:element ref="Pec"/>
        <xs:element ref="email"/>
        <xs:element ref="Tel"/>
        <xs:element ref="Fax"/>
        <xs:element ref="Ateco"/>
        <xs:element ref="Ote"/>
        <xs:element ref="Registro"/>
        <xs:element ref="Anno"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Titolo1sezAnagrafica" type="xs:string"/>
  <xs:element name="Titolo2sezAnagrafica" type="xs:string"/>
  <xs:element name="Cuaa" type="xs:string"/>
  <xs:element name="PartitaIva" type="xs:string"/>
  <xs:element name="IntestazionePartitaIva" type="xs:string"/>
  <xs:element name="Denominazione" type="xs:string"/>
  <xs:element name="FormaGiuridica" type="xs:string"/>
  <xs:element name="IndirizzoSedeLeg" type="xs:string"/>
  <xs:element name="Pec" type="xs:string"/>
  <xs:element name="email" type="xs:string"/>
  <xs:element name="Tel" type="xs:string"/>
  <xs:element name="Fax" type="xs:string"/>
  <xs:element name="Ateco" type="xs:string"/>
  <xs:element name="Ote" type="xs:string"/>
  <xs:element name="Registro" type="xs:string"/>
  <xs:element name="Anno" type="xs:string"/>
  <xs:element name="SezioneTitolare">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Visibility"/>
        <xs:element ref="TitoloSezTitolare"/>
        <xs:element ref="CodiceFiscaleTitolare"/>
        <xs:element ref="CognomeTitolare"/>
        <xs:element ref="NomeTitolare"/>
        <xs:element ref="TelefonoTitolare"/>
        <xs:element ref="MailTitolare"/>
        <xs:element ref="SessoTitolare"/>
        <xs:element ref="IndirizzoResidenzaTitolare"/>
        <xs:element ref="DataNascitaTitolare"/>
        <xs:element ref="LuogoNascitaTitolare"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TitoloSezTitolare" type="xs:string"/>
  <xs:element name="CodiceFiscaleTitolare" type="xs:string"/>
  <xs:element name="CognomeTitolare" type="xs:string"/>
  <xs:element name="NomeTitolare" type="xs:string"/>
  <xs:element name="TelefonoTitolare" type="xs:string"/>
  <xs:element name="MailTitolare" type="xs:string"/>
  <xs:element name="SessoTitolare" type="xs:string"/>
  <xs:element name="IndirizzoResidenzaTitolare" type="xs:string"/>
  <xs:element name="DataNascitaTitolare" type="xs:string"/>
  <xs:element name="LuogoNascitaTitolare" type="xs:string"/>
  <xs:element name="SezioneFirmatario">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Visibility"/>
        <xs:element ref="TitoloSezFirmatario"/>
        <xs:element ref="RuoloFirmatario"/>
        <xs:element ref="CodiceFiscaleFirmatario"/>
        <xs:element ref="CognomeFirmatario"/>
        <xs:element ref="NomeFirmatario"/>
        <xs:element ref="TelefonoFirmatario"/>
        <xs:element ref="MailFirmatario"/>
        <xs:element ref="SessoFirmatario"/>
        <xs:element ref="IndirizzoFirmatario"/>
        <xs:element ref="DataNascitaFirmatario"/>
        <xs:element ref="LuogoNascitaFirmatario"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TitoloSezFirmatario" type="xs:string"/>
  <xs:element name="RuoloFirmatario" type="xs:string"/>
  <xs:element name="CodiceFiscaleFirmatario" type="xs:string"/>
  <xs:element name="CognomeFirmatario" type="xs:string"/>
  <xs:element name="NomeFirmatario" type="xs:string"/>
  <xs:element name="TelefonoFirmatario" type="xs:string"/>
  <xs:element name="MailFirmatario" type="xs:string"/>
  <xs:element name="SessoFirmatario" type="xs:string"/>
  <xs:element name="IndirizzoFirmatario" type="xs:string"/>
  <xs:element name="DataNascitaFirmatario" type="xs:string"/>
  <xs:element name="LuogoNascitaFirmatario" type="xs:string"/>

  <!-- QUADRO CONTO CORRENTE  -->
  <xs:element name="QuadroContoCorrente">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Visibility"/>
        <xs:element ref="TitoloSezioneConto"/>
        <xs:element ref="ContoCorrente"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TitoloSezioneConto" type="xs:string"/>
  <xs:element name="ContoCorrente">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CifraControllo"/>
        <xs:element ref="CIN"/>
        <xs:element ref="ABI"/>
        <xs:element ref="CAB"/>
        <xs:element ref="NumeroContoCorrente"/>
        <xs:element ref="IBAN"/>
        <xs:element ref="Istituto"/>
        <xs:element ref="Agenzia"/>
        <xs:element ref="Intestazione"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CifraControllo" type="xs:string"/>
  <xs:element name="CIN" type="xs:string"/>
  <xs:element name="ABI" type="xs:string"/>
  <xs:element name="CAB" type="xs:string"/>
  <xs:element name="NumeroContoCorrente" type="xs:string"/>
  <xs:element name="IBAN" type="xs:string"/>
  <xs:element name="Istituto" type="xs:string"/>
  <xs:element name="Agenzia" type="xs:string"/>
  <xs:element name="Intestazione" type="xs:string"/>
 
  <!-- QUADRO ANTICIPO -->
  <xs:element name="QuadroAnticipo">
    <xs:complexType>
      <xs:sequence>
		<xs:element ref="SezioneTitoloAnticipo"/>
        <xs:element ref="SezioneAnticipo"/>
        <xs:element ref="SezioneFideiussione"/>
        <xs:element ref="SezioneBanca"/>
		<xs:element ref="SezioneAltroIstituto"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="SezioneTitoloAnticipo">
    <xs:complexType>
      <xs:sequence> 
		<xs:element ref="TitoloQuadroAnticipo"/> 
		<xs:element ref="Visibility"/> 
	  </xs:sequence>
    </xs:complexType>
  </xs:element> 
  <xs:element name="TitoloQuadroAnticipo" type="xs:string"/>  
  <xs:element name="Visibility" type="xs:boolean"/>
  <xs:element name="SezioneAnticipo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TitolosezAnticipo"/>
        <xs:element ref="ImportoInvestimento"/>
        <xs:element ref="ImportoAmmesso"/>
        <xs:element ref="ImportoContributo"/>
        <xs:element ref="PercentualeAnticipo"/>
		<xs:element ref="ImportoAnticipo"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TitolosezAnticipo" type="xs:string"/>
  <xs:element name="ImportoInvestimento" type="xs:decimal"/>
  <xs:element name="ImportoAmmesso" type="xs:decimal"/>
  <xs:element name="ImportoContributo" type="xs:decimal"/>
  <xs:element name="PercentualeAnticipo" type="xs:decimal"/>
  <xs:element name="ImportoAnticipo" type="xs:decimal"/>
  <xs:element name="SezioneFideiussione">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TitoloSezFideiussione"/>
        <xs:element ref="NumeroFideiussione"/>
        <xs:element ref="ImportoFideiussione"/>
        <xs:element ref="DataStipula"/>
        <xs:element ref="DataScadenza"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TitoloSezFideiussione" type="xs:string"/>
  <xs:element name="NumeroFideiussione" type="xs:string"/>
  <xs:element name="ImportoFideiussione" type="xs:decimal"/>
  <xs:element name="DataStipula" type="xs:date"/>
  <xs:element name="DataScadenza" type="xs:date"/>
  <xs:element name="SezioneBanca">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="VisibilitaBanca"/>
		<xs:element ref="TitoloSezBanca"/>
        <xs:element ref="DenominazioneBanca"/>
        <xs:element ref="SportelloIndCapComuneProv"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="VisibilitaBanca" type="xs:boolean"/>
  <xs:element name="TitoloSezBanca" type="xs:string"/>
  <xs:element name="DenominazioneBanca" type="xs:string"/>
  <xs:element name="SportelloIndCapComuneProv" type="xs:string"/>
<xs:element name="SezioneAltroIstituto">
    <xs:complexType>
      <xs:sequence>
		  <xs:element ref="VisibilitaIstituto"/>
        <xs:element ref="TitoloSezAltroIstituto"/>
        <xs:element ref="AltroIstituto"/>
        <xs:element ref="IndirizzoAltroIstituto"/>
		<xs:element ref="AltroIstitutoCapComuneProv"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="VisibilitaIstituto" type="xs:boolean"/>
  <xs:element name="TitoloSezAltroIstituto" type="xs:string"/>
  <xs:element name="AltroIstituto" type="xs:string"/>
  <xs:element name="IndirizzoAltroIstituto" type="xs:string"/>  
  <xs:element name="AltroIstitutoCapComuneProv" type="xs:string"/>
 
 <!-- Quadro Colture Aziendali -->
  
  <xs:element name="QuadroColtureAziendali">
    <xs:complexType>
      <xs:sequence>
		<xs:element ref="Visibility"/>
	    <xs:element ref="TitoloSezioneColtureAziendali"/>
		<xs:element ref="SezDatiColture"/>
		<xs:element ref="SezNulla"/>
       </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TitoloSezioneColtureAziendali" type="xs:string"/>
  
  <xs:element name="SezDatiColture">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="SezRiepilogo"/>
			<xs:element ref="SezElencoColture"/>
			<xs:element ref="VisibilityDati"/>
		</xs:sequence>
    </xs:complexType>
  </xs:element>	
  
  <xs:element name="VisibilityDati" type="xs:boolean"/>
  
  <xs:element name="SezRiepilogo">
	<xs:complexType>
	 <xs:sequence>
		<xs:element ref="TotSup"/>
		<xs:element ref="PLVOrdinaria"/>
		<xs:element ref="PLVEffettiva"/>
		<xs:element ref="PercDanno"/>
	 </xs:sequence>
	</xs:complexType>
  </xs:element>
	
	<xs:element name="TotSup" type="string"/>
	<xs:element name="PLVOrdinaria" type="string"/>
	<xs:element name="PLVEffettiva" type="string"/>
	<xs:element name="PercDanno" type="string"/>
	
	
  <xs:element name="SezElencoColture">
	<xs:complexType>
	 <xs:sequence>
		<xs:element ref="TabColtureAziendali"/>
	 </xs:sequence>
	</xs:complexType>
  </xs:element>
  
  <xs:element name="TabColtureAziendali">
    <xs:complexType>
	 <xs:sequence>
		<xs:element ref="RigaColtureAziendali" maxOccurs="unbounded"/>
	 </xs:sequence>
	</xs:complexType>
  </xs:element>
  
  <xs:element name="RigaColtureAziendali">
	<xs:complexType>
	 <xs:sequence>
		<xs:element ref="UbicazioneTerreno"/>
		<xs:element ref="Utilizzo"/>
		<xs:element ref="Superficie"/>
		<xs:element ref="QuintaliHa"/>
		<xs:element ref="TotQliOrd"/>
		<xs:element ref="PrezzoQleOrd"/>
		<xs:element ref="TotEuroOrd"/>
		<xs:element ref="TotQliEff"/>
		<xs:element ref="PrezzoQleEff"/>
		<xs:element ref="TotEuroEff"/>
		<xs:element ref="Danno"/>
	 </xs:sequence>
	</xs:complexType>
  </xs:element>
	
	<xs:element name="UbicazioneTerreno" type="string"/>
	<xs:element name="Utilizzo" type="string"/>
	<xs:element name="Superficie" type="string"/>
	<xs:element name="QuintaliHa" type="string"/>
	<xs:element name="TotQliOrd" type="string"/>
	<xs:element name="PrezzoQleOrd" type="string"/>
	<xs:element name="TotEuroOrd" type="string"/>
	<xs:element name="TotQliEff" type="string"/>
	<xs:element name="PrezzoQleEff" type="string"/>
	<xs:element name="TotEuroEff" type="string"/>
	<xs:element name="Danno" type="string"/>
	
	
  
  <xs:element name="SezNulla">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="VisibilitySezNulla"/>
		</xs:sequence>
	</xs:complexType>
  </xs:element>
  
  <xs:element name="VisibilitySezNulla" type="boolean"/>
 
 
 <!-- Quadro Interventi Infrastrutture -->
  
  <xs:element name="QuadroInterInfrastrutture">
    <xs:complexType>
      <xs:sequence>
		<xs:element ref="Visibility"/>
	    <xs:element ref="TitoloSezioneInterInfrastrutture"/>
		<xs:element ref="InterInfrastrutture"/>	
		<xs:element ref="Localizzazione"/>
       </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TitoloSezioneInterInfrastrutture" type="xs:string"/>
  
  <xs:element name="InterInfrastrutture">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="TabInterInfrastrutture"/>
		</xs:sequence>
    </xs:complexType>
  </xs:element>	
 
  <xs:element name="TabInterInfrastrutture">
	<xs:complexType>
	 <xs:sequence>
		<xs:element ref="RigaInterInfrastrutture" maxOccurs="unbounded"/>
		<xs:element ref="RigaTotale"/>
	 </xs:sequence>
	</xs:complexType>
  </xs:element>
	<xs:element name="RigaInterInfrastrutture">
    <xs:complexType>
      <xs:sequence>
		<xs:element ref="ProgressivoIntervento"/>
		<xs:element ref="Intervento"/>
		<xs:element ref="DescIntervento"/>
		<xs:element ref="ImportoIntervento"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ProgressivoIntervento" type="xs:string"/>
  <xs:element name="Intervento" type="xs:string"/>
  <xs:element name="DescIntervento" type="xs:string"/>
  <xs:element name="ImportoIntervento" type="xs:string"/>
  
  <xs:element name="RigaTotale">
    <xs:complexType>
      <xs:sequence>
		<xs:element ref="TotImporto"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TotImporto" type="string"/>
  
  <xs:element name="Localizzazione">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="VisibilityLoc"/>
			<xs:element ref="TabLoc"/>
		</xs:sequence>
	</xs:complexType>
  </xs:element>
  
  <xs:element name="TabLoc">
	<xs:complexType>
	 <xs:sequence>
		<xs:element ref="RigaLoc" maxOccurs="unbounded"/>
	 </xs:sequence>
	</xs:complexType>
  </xs:element>
	<xs:element name="RigaLoc">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Comune"/>
		<xs:element ref="Progressivo"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Comune" type="xs:string"/>
  <xs:element name="Progressivo" type="string"/>
  
  <xs:element name="VisibilityLoc" type="boolean"/>
 
 
 <!-- Quadro PLV Vegetale -->
  
  <xs:element name="QuadroPLVVegetale">
    <xs:complexType>
      <xs:sequence>
		<xs:element ref="Visibility"/>
	    <xs:element ref="TitoloSezionePLVVegetale"/>
		<xs:element ref="PLVVegetale"/>	
		<xs:element ref="SezioneNulla"/>
       </xs:sequence>
    </xs:complexType>		
  </xs:element>
  <xs:element name="TitoloSezionePLVVegetale" type="xs:string"/>
  
  <xs:element name="PLVVegetale">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="TabPLV"/>
			<xs:element ref="VisibilityPLVVegetale"/>
		</xs:sequence>
    </xs:complexType>
  </xs:element>	
  
  <xs:element name="VisibilityPLVVegetale" type="xs:boolean"/>
  
  <xs:element name="TabPLV">
	<xs:complexType>
	 <xs:sequence>
		<xs:element ref="RigaPLV" maxOccurs="unbounded"/>
	 </xs:sequence>
	</xs:complexType>
  </xs:element>
	<xs:element name="RigaPLV">
    <xs:complexType>
      <xs:sequence>
		<xs:element ref="Utilizzo"/>
        <xs:element ref="Superficie"/>
		<xs:element ref="Produzione"/>
		<xs:element ref="GGLavorative"/>
		<xs:element ref="UF"/>
		<xs:element ref="ReimpieghiQ"/>
		<xs:element ref="ReimpieghiUF"/>
		<xs:element ref="PLV"/>		
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Utilizzo" type="xs:string"/>
  <xs:element name="Superficie" type="xs:string"/>
  <xs:element name="Produzione" type="xs:string"/>
  <xs:element name="GGLavorative" type="xs:string"/>
  <xs:element name="UF" type="xs:string"/>
  <xs:element name="ReimpieghiQ" type="xs:string"/>
  <xs:element name="ReimpieghiUF" type="xs:string"/>
  <xs:element name="PLV" type="xs:string"/>
  
  <xs:element name="SezioneNulla">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="VisibilityTestoNullo"/>
		</xs:sequence>
	</xs:complexType>
  </xs:element>
  
  <xs:element name="VisibilityTestoNullo" type="boolean"/>
  
 
 <!-- Quadro PLV Zootecnica -->
  
  <xs:element name="QuadroPLVZootecnica">
    <xs:complexType>
      <xs:sequence>
		<xs:element ref="Visibility"/>
	    <xs:element ref="TitoloSezionePLVZootecnica"/>
		<xs:element ref="PLVZootecnica"/>	
		<xs:element ref="SezioneNulla"/>
       </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TitoloSezionePLVZootecnica" type="xs:string"/>
  
  <xs:element name="PLVZootecnica">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="TabPLVZootecnica"/>
			<xs:element ref="VisibilityPLVZootenica"/>
		</xs:sequence>
    </xs:complexType>
  </xs:element>	
 
  <xs:element name="VisibilityPLVZootenica" type="xs:boolean"/>
  <xs:element name="TabPLVZootecnica">
	<xs:complexType>
	 <xs:sequence>
		<xs:element ref="RigaPLVZootecnica" maxOccurs="unbounded"/>
	 </xs:sequence>
	</xs:complexType>
  </xs:element>
